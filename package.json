{
  "name": "aichat",
  "version": "0.0.10",
  "description": "Chat interface to LLMs",
  "type": "module",
  "main": "server.ts",
  "scripts": {
    "start": "node --experimental-strip-types server.ts",
    "test": "node --experimental-strip-types --test test/*.test.ts",
    "lint": "npx eslint . --ext .ts --fix --max-warnings 0",
    "format": "npx prettier *.ts lib/*.ts scripts/*.ts test/*.ts --write",
    "typecheck": "npx tsc --noEmit",
    "sanity-check": "node --experimental-strip-types scripts/sanity-check.ts",
    "add-model": "node --experimental-strip-types scripts/add-model.ts",
    "check-models": "node --experimental-strip-types scripts/check-models.ts"
  },
  "author": "IanKulin",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.1",
    "@ai-sdk/deepseek": "^1.0.3",
    "@ai-sdk/google": "^2.0.3",
    "@ai-sdk/openai": "^2.0.7",
    "@iankulin/logger": "^1.1.0",
    "@openrouter/ai-sdk-provider": "^1.1.2",
    "@types/marked": "^5.0.2",
    "ai": "^5.0.8",
    "better-sqlite3": "^12.2.0",
    "dompurify": "^3.2.6",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "highlight.js": "^11.11.1",
    "marked": "^16.1.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^4.17.23",
    "@types/node": "^24.0.3",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.5",
    "prettier": "^3.0.0",
    "supertest": "^7.1.1"
  }
}

{
  "name": "aichat",
  "version": "0.0.2",
  "description": "Chat interface to LLMs",
  "type": "module",
  "main": "server.ts",
  "scripts": {
    "start": "node --experimental-strip-types server.ts",
    "dev": "node --experimental-strip-types --watch server.ts",
    "test": "echo 'No tests available'",
    "test:watch": "echo 'No tests available'",
    "lint": "npx eslint *.ts lib/*.ts --fix --max-warnings 0",
    "format": "npx prettier *.ts lib/*.ts --write",
    "typecheck": "npx tsc --noEmit"
  },
  "author": "IanKulin",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/deepseek": "^0.2.15",
    "@ai-sdk/google": "^1.2.22",
    "@ai-sdk/openai": "^1.3.23",
    "ai": "^4.3.17",
    "dotenv": "^16.5.0",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.23",
    "@types/node": "^24.0.3",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "supertest": "^7.1.1"
  }
}
